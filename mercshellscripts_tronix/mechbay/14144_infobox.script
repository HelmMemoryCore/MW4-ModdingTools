					print3d_margins = 182, 530 to 640, 595
					print3d_position = 182, 534
				}

				// boundaries refer to temp_mech_id upper and lower bounds
				if (id >=FIRST_WEAPON_INFO_ID && id <= LAST_WEAPON_INFO_ID)
				{
					setpencolor(0xaaffffff)
					drawline 175, 535 to 175, 590

					callback($$GetWeaponStats$$, id, damage, range, heat, weapon_recycle, weapon_weight )
					//damage = 0
					//range = 0
					//heat = 0
					//weapon_recycle = 0

					print3d_attributes	= regfont, COMSTAR_GREEN, 1, 1, 1, 0, 0, just_right
