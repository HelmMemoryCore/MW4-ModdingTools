	GUI_INIT
	{
		displayMissionData = false
	}

	GUI_DRAW
	{
		if ( showinfo == true && enabled==true )
		{
			// boundaries refer to temp_mech_id upper and lower bounds
			if (displayMissionData == true && id > 0 )
			{
				int iDmy
				string szDmy
				string szTitle
				string szEmployer
				string szAlignment
				string szPayment
				string szMissionType
				int heatSinkEfficiency
				int missionBtnState
				region = 10, 525 to 570, 590

				if ( callback($$GetMissionParameters$$, id, szEmployer, szAlignment, szPayment, heatSinkEfficiency, szMissionType ) )
				{
					callback($$Shell_CallbackHandler$$,ShellCampaignGetMissionRegionDATA, id,iDmy,iDmy,iDmy, iDmy,missionBtnState,iDmy,szDmy,szTitle,szDmy)
					if ( missionBtnState == 1 )
					{
						setpencolor(0xaaffffff)
						drawline 200, 505 to 200, 580

						print3d_attributes	= regfont, COMSTAR_GREEN, 1, 1, 1, 0, 0, just_right
						print3d_margins		= 80, 500 to 195, 595
						print3d_position	= 80, 505
						print3d szMissionType
						print3d_position	= 80, 520
						print3d szEmployer
						print3d_position	= 80, 535
						print3d szAlignment
						print3d_position	= 80, 550
						print3d szPayment
						print3d_position	= 80, 565
						print3d conv$( heatSinkEfficiency ) "%"

						print3d_attributes	= regfont, COMSTAR_GREEN, 1, 1, 1, 0, 0
						print3d_margins		= 15, 500 to 195, 595
						print3d_position	= 15, 505
						print3d localize$( IDS_MISSIONTYPE )
						print3d_position	= 15, 520
						print3d localize$( IDS_EMPLOYER )
						print3d_position	= 15, 535
						print3d localize$( IDS_ALIGNMENT )
						print3d_position	= 15, 550
						print3d localize$( IDS_PAYMENT )
						print3d_position	= 15, 565
						print3d localize$( IDS_HSEFFICIENCY )

