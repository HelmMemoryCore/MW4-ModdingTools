					print3d damage
					print3d_position	= 100, 564
					print3d heat
					print3d_position	= 100, 578
					print3d weapon_recycle
					print3d_attributes	= regfont, COMSTAR_GREEN, 1, 1, 1, 0, 0
					print3d_margins		= 15, 524 to 150, 595
					print3d_position	= 15, 536
					print3d localize$(IDS_ML_WEAPONRANGE_INFO)
					print3d_position	= 15, 550
					print3d localize$(IDS_ML_WEAPONDAMAGE_INFO)
					print3d_position	= 15, 564
					print3d localize$(IDS_ML_WEAPONHEAT_INFO)
					print3d_position	= 15, 578
					print3d localize$(IDS_ML_WEAPONRECYCLE_INFO)

					print3d_attributes = regfont, COMSTAR_GREEN, 1, 0, 1, 0, 0, just_right
					print3d_margins = 400, 515 to 630, 560
					print3d_position = 400, 534
					ton_times_ten = weapon_weight * 10.0f
					ton_int_times_ten = weapon_weight * 10
					ton_decimal_times_ten = ton_times_ten - ton_int_times_ten
					print3d localize$( IDS_FM_WEAPONWEIGHT ) "   " conv$(weapon_weight) "." conv$(ton_decimal_times_ten)

					print3d_attributes = regfont, COMSTAR_GREEN, 1, 1, 1, 0, 0
					print3d_margins = 182, 530 to 640, 595
					print3d_position = 182, 534
				}
			}
			else
			{
				print3d_attributes = regfont, COMSTAR_GREEN, 1, 1, 1, 0, 0
				print3d_margins = 15, 530 to 640, 595
				print3d_position = 15, 538
			}

			if id < max_infomessage
			{
				if id > -1
