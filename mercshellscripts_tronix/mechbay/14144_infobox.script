					{
						print3d_attributes = regfont, COMSTAR_GREEN, 1, 1, 1, 0, 0
						print3d_margins = 15, 505 to 570, 600
						print3d_position = 15, 505
						print3d infomessage[id]
					}
				}
				return
			}

			if ((id > -1) && (id <= 146)) OR ((id > 550) && (id <= 580)) OR ((id > 599) && (id <= 622)) OR (id >=325 && id <= 399) OR (id >=FIRST_LANCEMATE_INFO_ID && id <= LAST_LANCEMATE_INFO_ID)
			{

				if (id > -1) && (id <= 146) OR ((id > 550) && (id <= 580)) OR ((id > 599) && (id <= 622))
				{
